{
  "configurations": [
    {
      "name": "(lldb) Attach",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/kernel.bin",
      "cwd": ".",
      "launchCompleteCommand": "exec-continue",

      "MIMode": "lldb",
      "miDebuggerServerAddress": "localhost:1234",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
